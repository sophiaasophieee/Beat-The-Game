{
    "manifest_version": 3,
    "name": "Awesome",
    "version": "1.0",
    "description": "Does a super awesome thing",
    "permissions": [
        "activeTab",
        "declarativeContent"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["jscript.js"],
            "run_at": "document_end"
        }
    ],
    "icons": {
        "48": "icon.png"
    }
}